[submodule "riscv-gnu-toolchain"]
	path = riscv-gnu-toolchain
	url = https://github.com/riscv/riscv-gnu-toolchain
[submodule "riscv-pk"]
	path = riscv-pk
	url = https://github.com/iaclab-hpme/riscv-pk
[submodule "sdk"]
	path = sdk
	url = https://github.com/iaclab-hpme/keystone-sdk
	branch = dev-hpme
[submodule "linux-keystone-driver"]
